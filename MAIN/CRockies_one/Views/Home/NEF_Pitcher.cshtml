@section scripts{

    <script type="text/javascript">
        $(document).ready(function () {
            $('#saveAllTableFixed').hide();
            //Check to see if the window is top if not then display button
            $(window).scroll(function () {
                if ($(this).scrollTop() > 250) {//155
                    $("#saveAll").appendTo($("#fixedSaveAllBtnCont"));
                    $("#cancelAll").appendTo($("#fixedSaveAllBtnCont"));
                    $('#saveAllTableFixed').show();
                } else {
                    $("#saveAll").appendTo($("#saveAllTableCell"));
                    $("#cancelAll").appendTo($("#saveAllTableCell"));
                    $('#saveAllTableFixed').hide();
                }
            });
        });
        function getParameterByName(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
            return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        };
        $("#cancelAll").click(function () {
            var _teamid = getParameterByName("_teamid");
            location.replace('/Home/NotesEntryForm?_teamid=' + _teamid);
